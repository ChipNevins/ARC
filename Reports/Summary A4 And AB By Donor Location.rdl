  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=CHIP-HP7;Initial Catalog=HLA</ConnectString>
        <Prompt>Specify a user name and password for data source DataSource1:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>7033f367-00e3-4a22-bfb0-f00022b7fedf</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
       TESTCODE.strcodeshortname TestCodeAbbr,
       IsNull(DONORLOCATION.strcodeshortname,' - not entered -') DonorLocationName,
       hla_specimen.strspecimennum SpecimenNum,
       REGCAT.strcodename RegCatName,
       hla_person.strrefnum,
       1 QTY
  from hla_person 
    left outer join hla_code as REGCAT on hla_person.cdregcat = REGCAT.pkcodenumber and REGCAT.pkcodetype = 'REG CATEGORY'
    left outer join hla_code as DONORLOCATION on hla_person.cddonorlocation = DONORLOCATION.pkcodenumber and DONORLOCATION.pkcodetype = 'DONOR LOCATION',
       hla_specimen,
       HLA_Test
    left outer join hla_code as TESTCODE on HLA_Test.cdtestcode = TESTCODE.pkcodenumber and TESTCODE.pkcodetype = 'TEST CODE'
 where CAST(hla_test.dtorderdate AS DATE) between @FromDate and @ToDate
   and hla_specimen.fkpid = hla_person.pkpid
   and hla_test.pksid = hla_specimen.pksid
   and hla_specimen.pksid = HLA_Test.pksid
   and TESTCODE.strcodeshortname IN ('A4','AB')
   and REGCAT.strcodename = 'DONOR'
   order by TestCodeAbbr,DonorLocationName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SpecimenNum">
          <DataField>SpecimenNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TestCodeAbbr">
          <DataField>TestCodeAbbr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DonorLocationName">
          <DataField>DonorLocationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RegCatName">
          <DataField>RegCatName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="strrefnum">
          <DataField>strrefnum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QTY">
          <DataField>QTY</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <InterpretSubtotalsAsDetails>False</InterpretSubtotalsAsDetails>
